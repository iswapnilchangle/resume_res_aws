<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Swapnil Changle - Resume</title>
  <style>
    /* Add your custom CSS styles here */
    body {
      font-family: Arial, sans-serif;
      line-height: 1.6;
      margin: 0;
      padding: 0;
    }
    h1, h2, h3, h4, h5, h6 {
      margin-bottom: 0.5rem;
    }
    p {
      margin-bottom: 1rem;
    }
    ul {
      list-style: none;
      padding: 0;
    }
    li {
      margin-bottom: 0.5rem;
    }
    a {
      color: #0066cc;
      text-decoration: none;
    }
    a:hover {
      text-decoration: underline;
    }
    @media (max-width: 768px) {
      /* Add your mobile-specific CSS styles here */
    }
  </style>
</head>
<body>
  <header>
    <h1><%= name %></h1>
    <p><%= phone %> ⋄ <%= city %></p>
    <p><a href="mailto:<%= email %>"><%= email %></a> ⋄ <a href="<%= linkedIn %>" target="_blank"><%= linkedIn %></a> ⋄ <a href="<%= website %>" target="_blank"><%= website %></a></p>
  </header>
  <main>
    <section>
      <h2>Objective</h2>
      <p><%= objective %></p>
    </section>
   <section>
      <h2>Skills</h2>
      <ul>
        <% skills.forEach(skill => { %>
          <li><%= skill.name %> (<%= skill.level %>)</li>
        <% }); %>
      </ul>
    </section>
    <section>
      <h2>Experience</h2>
      <% experience.forEach(job => { %>
        <h3><%= job.position %></h3>
        <p><%= job.company %> - <%= job.location %> (<%= job.duration %>)</p>
        <ul>
          <% job.achievements.forEach(achievement => { %>
            <li><%= achievement %></li>
          <% }); %>
        </ul>
      <% }); %>
    </section>
    <section>
      <h2>Education</h2>
      <% education.forEach(degree => { %>
        <h3><%= degree.degree %></h3>
        <p><%= degree.college %> (<%= degree.duration %>)</p>
      <% }); %>
    </section>
    <section>
      <h2>Certifications</h2>
      <% certifications.forEach(certification => { %>
        <h3><%= certification.name %></h3>
        <p>Issued by: <%= certification.issuedBy %> (<%= certification.date %>)</p>
      <% }); %>
    </section>
    <section>
      <h2>Projects</h2>
      <% projects.forEach(project => { %>
        <h3><%= project.name %></h3>
        <p><%= project.description %></p>
        <ul>
          <% project.achievements.forEach(achievement => { %>
            <li><%= achievement %></li>
          <% }); %>
        </ul>
      <% }); %>
    </section>
    <section>
      <h2>Extra-Curricular Activities</h2>
      <% extraCurricular.forEach(activity => { %>
        <h3><%= activity.activity %></h3>
        <p><%= activity.description %></p>
      <% }); %>
    </section>
    <section>
      <h2>Leadership</h2>
      <% leadership.forEach(role => { %>
        <h3><%= role.role %></h3>
        <p><%= role.company %></p>
        <ul>
          <% role.achievements.forEach(achievement => { %>
            <li><%= achievement %></li>
          <% }); %>
        </ul>
      <% }); %>
    </section>
  </main>
</body>
</html>
